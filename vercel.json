{
    "rewrites": [
        {
            "source": "/local/(.*)",
            "destination": "/api/server/$1"
        },
        {
            "source": "/api/:path*",
            "destination": "https://raider.io/:path*"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}