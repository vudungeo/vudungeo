{
    "rewrites": [
        {
            "source": "/local/:path*",
            "destination": "/api/server"
        },
        {
            "source": "/api/:path*",
            "destination": "https://raider.io/api/:path*"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}